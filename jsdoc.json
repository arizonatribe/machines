{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)",
    "include": ["src", "package.json", "README.md"]
  },
  "templates": {
    "name": "machines",
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "useCollapsibles": true,
    "footerText": "Simplified state machines",
    "logo": {
      "url": "https://arizonatribe.github.io/machines/styles/logo.png",
      "width": "228px",
      "height": "229px",
      "link": "https://github.com/arizonatribe/machines"
    },
    "default": {
      "staticFiles": {
        "include": ["assets/"]
      }
    },
    "css": [
      "styles/after.css"
    ],
    "tabNames": {
      "api": "API",
      "tutorials": "Examples"
    }
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "recurse": true,
    "template": "node_modules/tui-jsdoc-template"
  }
}
