{
  "name": "machines",
  "description": "Simplified state machines",
  "main": "src/index.js",
  "version": "1.2.5",
  "private": true,
  "license": "MIT",
  "author": "David Nunez <arizonatribe@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/arizonatribe/machines.git"
  },
  "keywords": [
    "state machines",
    "javascript",
    "state",
    "machines",
    "object",
    "immutability",
    "compose",
    "redux",
    "middleware"
  ],
  "watch": {
    "docs": "{assets,src}/*"
  },
  "scripts": {
    "docs": "jsdoc --configure jsdoc.json",
    "docs:server": "live-server --open=docs --watch=assets,docs --port=5000",
    "start": "npm run test:watch",
    "prestart:docs": "npm run docs",
    "start:docs": "concurrently \"npm-watch\" \"npm run docs:server\"",
    "test:watch": "nodemon test/index.js",
    "test": "tape --require=@babel/register test/index.js | faucet",
    "lint": "eslint src test",
    "prepare": "npm run build",
    "build": "node scripts/run"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/node": "7.0.0",
    "@babel/plugin-external-helpers": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-stage-0": "7.0.0",
    "@babel/register": "7.0.0",
    "@babel/runtime": "7.0.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "babel-eslint": "10.0.1",
    "babel-plugin-add-module-exports": "^1.0.4",
    "chalk": "2.4.1",
    "concurrently": "^6.2.0",
    "cross-env": "5.1.4",
    "cross-spawn": "6.0.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-flowtype": "3.4.1",
    "eslint-plugin-import": "2.14.0",
    "faucet": "0.0.4",
    "fs-extra": "7.0.0",
    "fs-readdir-recursive": "1.1.0",
    "glob": "7.1.1",
    "jsdoc": "^3.6.7",
    "jsdoc-export-default-interop": "^0.3.1",
    "live-server": "1.2.1",
    "nodemon": "^2.0.12",
    "npm-watch": "^0.10.0",
    "rollup": "~2.38.0",
    "rollup-plugin-terser": "^7.0.0",
    "tape": "4.9.1",
    "tui-jsdoc-template": "1.2.2"
  }
}
